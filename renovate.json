{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "foo.yml"
      ],
      "matchStrings": [
        "tfLintVersion: (?<currentValue>.*?)\\n"
      ],
      "depNameTemplate": "terraform-linters/tflint",
      "datasourceTemplate": "github-releases"
    }
  ]
}
